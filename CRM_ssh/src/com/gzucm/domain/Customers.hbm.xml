<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.gzucm.domain" >
	<class name="Customers" table="sys_customer" >
		<id name="customer_id"  >
			<generator class="native"></generator>
		</id>
		<property name="customer_code"  ></property>
		<property name="customer_name"  ></property>
		<property name="customer_password"  ></property>
		<property name="customer_state"  ></property>
	
	</class>
</hibernate-mapping>